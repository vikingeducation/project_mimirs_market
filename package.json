    "body-parser": "^1.18.2",
    "cookie-session": "^1.3.2",
    "express": "^4.16.0",
    "express-handlebars": "^3.0.0",
    "faker": "^4.1.0",
    "mongoose": "^4.11.13",
    "morgan": "^1.9.0",
